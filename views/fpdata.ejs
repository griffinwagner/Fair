      <script>

      var MarkerAnnotation = mapkit.MarkerAnnotation,
            clickAnnotation;
        var vero = new mapkit.Coordinate(27.6386, -80.3973),
            work = new mapkit.Coordinate(27.475569444444442, -80.3266);


        mapkit.init({
            authorizationCallback: function(done) {
                done('eyJhbGciOiJFUzI1NiIsInR5cCI6IkpXVCIsImtpZCI6IlFZUFg5SFlGSzIifQ.eyJpc3MiOiJCOUo1V0tSNlhBIiwiaWF0IjoxNTQyOTAyMzI4LCJleHAiOjEwNTQyOTAyMzI4fQ.G32LeRw7eHKuxGEcKqWvwewW2JTso1SEHZoEjEiscqXqN83w1lkDKsAYq503SQwFnB7qHZzyb0V_fmW1J4I2Dw');
            }
        });

        // var Cupertino = new mapkit.CoordinateRegion(
        // new mapkit.Coordinate(37.3316850890998, -122.030067374026),
        // new mapkit.CoordinateSpan(0.167647972, 0.354985255)
        // );
        //
        // var map = new mapkit.Map("map");
        // map.region = Cupertino;
        // map.mapType = 'satellite'

        var map = new mapkit.Map("map");
        map.mapType = "hybrid"

        // Setting properties on creation:
        var veroAnnotation = new MarkerAnnotation(vero, { color: "#f4a56d", title: "Vero Beach", glyphText: "üèñ" });

        // Setting properties after creation:
        var workAnnotation = new MarkerAnnotation(work);
        workAnnotation.color = "#969696";
        workAnnotation.title = "Site";
        workAnnotation.subtitle = "IRL-LP";
        workAnnotation.selected = "true";
        workAnnotation.glyphText = "‚ôªÔ∏è";

        // Add and show both annotations on the map
        map.showItems([veroAnnotation, workAnnotation]);

        // Drop an annotation where a Shift-click is detected:
        map.element.addEventListener("click", function(event) {
            if(!event.shiftKey) {
                return;
            }

            if(clickAnnotation) {
                map.removeAnnotation(clickAnnotation);
            }

            var coordinate = map.convertPointOnPageToCoordinate(new DOMPoint(event.pageX, event.pageY));
            clickAnnotation = new MarkerAnnotation(coordinate, {
                title: "Click!",
                color: "#c969e0"
            });
            map.addAnnotation(clickAnnotation);
        });
      </script>



